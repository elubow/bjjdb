<div class="col">

  <h1 class="mt-4">Notebook</h1>
<table "table table-hover">
  <thead>
    <tr>
      <th>User</th>
      <th>Link</th>
      <th>Body</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @private_notes.each do |private_note| %>
      <tr>
        <td><%= link_to private_note.user.name, private_note.user.name %></td>
        <td><%= link_to private_note.link.title, private_note.link %></td>
        <td><%= private_note.body %></td>
        <td><%= private_note.title %></td>
        <td><%= link_to 'Show', private_note %></td>
        <td><%= link_to 'Edit', edit_private_note_path(private_note) %></td>
        <td><%= link_to 'Destroy', private_note, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>

